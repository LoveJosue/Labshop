{
  "version": 2,
  "builds": [
    { "src": "Labshop/vite.config.js", "use": "@vercel/static-build", "config": { "distDir": "Labshop/dist" } },
    { "src": "Server/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/Server/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/Labshop/dist/$1" }
  ]
}